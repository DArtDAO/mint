{
  "name": "nextjs-ethereum-starter-hardhat",
  "author": "@huntarosan",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build:ts": "tsc -p tsconfig.json",
    "chain": "npx hardhat node --network hardhat",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat --network localhost run scripts/deploy.ts",
    "deploy:rinkeby": "npx hardhat --network rinkeby run scripts/deploy.ts",
    "test": "npx hardhat test --network hardhat",
    "console": "npx hardhat console --network localhost"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@typechain/ethers-v5": "^8.0.1",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.12.2",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.0",
    "hardhat": "^2.6.7",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@openzeppelin/contracts": "^4.3.2",
    "base64-sol": "^1.0.1"
  }
}
